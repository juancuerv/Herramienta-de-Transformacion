<?xml version="1.0" encoding="UTF-8"?>
<hyperledgerGo:SmartContract
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:hyperledgerGo="http://www.example.org/hyperledgerGo"
    xsi:schemaLocation="http://www.example.org/hyperledgerGo hyperledgerGo.ecore"
    SCName="contractEhr"
    FileName="ehr">
  <packages
      Name="main"/>
  <structs Name="EHRContract">
    <variables
        Name="contractapi.Contract"
        IsAnonym="true"/>
  </structs>
  <structs Name="Patient">
    <variables
        Name="ID">
      <tags
          Name="json:&quot;ID&quot;"/>
    </variables>
    <variables
        Name="Name">
      <tags
          Name="json:&quot;name&quot;"/>
    </variables>
    <variables
        Name="TypeId">
      <tags
          Name="json:&quot;typeId&quot;"/>
    </variables>
    <variables
        Name="Phone"
        Type="int">
      <tags
          Name="json:&quot;phone&quot;"/>
    </variables>
    <variables
        Name="Address">
      <tags
          Name="json:&quot;address&quot;"/>
    </variables>
    <variables
        Name="Email">
      <tags
          Name="json:&quot;email&quot;"/>
    </variables>
    <variables
        Name="ClinicalData">
      <tags
          Name="json:&quot;clinicalData&quot;"/>
    </variables>
  </structs>
  <functions
      Name="CreatePatient"
      ReturnType="error">
    <parameters
        Name="ctx"
        Type="contractapi.TransactionContextInterface"/>
    <parameters
        Name="ID"/>
    <parameters
        Name="Name"/>
    <parameters
        Name="TypeId"/>
    <parameters
        Name="Phone"
        Type="int"/>
    <parameters
        Name="Address"/>
    <parameters
        Name="Email"/>
    <parameters
        Name="ClinicalData"/>
    <typefunctions
        VarName="ec"
        StructName="EHRContract"/>
  </functions>
  <functions
      Name="PatientExists"
      ReturnType="bool, error">
    <parameters
        Name="ctx"
        Type="contractapi.TransactionContextInterface"/>
    <parameters
        Name="ID"/>
    <typefunctions
        VarName="ec"
        StructName="EHRContract"/>
  </functions>
  <functions
      Name="UpdatePatient"
      ReturnType="error">
    <parameters
        Name="ctx"
        Type="contractapi.TransactionContextInterface"/>
    <parameters
        Name="ID"/>
    <parameters
        Name="ClinicalData"/>
    <typefunctions
        VarName="ec"
        StructName="EHRContract"/>
  </functions>
  <functions
      Name="DeletePatient"
      ReturnType="error">
    <parameters
        Name="ctx"
        Type="contractapi.TransactionContextInterface"/>
    <parameters
        Name="ID"/>
    <typefunctions
        VarName="ec"
        StructName="EHRContract"/>
  </functions>
  <functions
      Name="ReadPatient"
      ReturnType="*Patient, error">
    <parameters
        Name="ctx"
        Type="contractapi.TransactionContextInterface"/>
    <parameters
        Name="ID"/>
    <typefunctions
        VarName="ec"
        StructName="EHRContract"/>
  </functions>
  <functions
      Name="main">
    <localvariables
        Name="EHRContract"/>
  </functions>
</hyperledgerGo:SmartContract>
